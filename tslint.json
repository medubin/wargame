{
  "defaultSeverity": "error",
  "extends": [
      "tslint-config-airbnb"
  ],
  "jsRules": {},
  "rules": {
    "semicolon": [true, "never"],
    "import-name": false,
    "no-boolean-literal-compare": false,
    "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore"]
  },
  "rulesDirectory": []
}